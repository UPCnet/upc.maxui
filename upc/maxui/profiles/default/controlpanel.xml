<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone"
    purge="False">

    <configlet
        title="MAX UI Settings"
        action_id="maxui"
        appId="upc.maxui"
        category="Products"
        condition_expr=""
